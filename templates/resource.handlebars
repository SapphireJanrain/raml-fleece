{{#emptyResourceCheck}}
    <div class="panel panel-default">
        <!-- <div class="panel-heading">-->
        <div class="panel-heading" data-toggle="collapse" id="panel_header_{{uniqueId}}" href="#panel_{{uniqueId}}">
            <h4 class="panel-title">
                <a class="collapsed" data-toggle="collapse" href="#panel_{{uniqueId}}">
                    <span class="parent">{{parentUrl}}</span>{{relativeUri}}
                </a>

                <span class="methods">
                    {{#methods}}
                        <span class="badge badge_{{method}}">{{method}}{{lock securedBy}}</span>
                    {{/methods}}
                </span>
            </h4>
        </div>
        <div id="panel_{{uniqueId}}" class="panel-collapse collapse">
            {{#methods}}
                <div class="panel-body">
                    <h4>
                        <span class="badge badge_{{method}}">{{method}}{{lock securedBy}}</span>
                        <a
                            id="{{method}}_{{../uniqueId}}"
                            data-anchor
                            data-method="{{method}}"
                            data-endpoint="{{../uniqueId}}"
                            href="#{{method}}:{{../uniqueId}}"
                            class="parent">
                                {{../parentUrl}}{{../relativeUri}}
                        </a>
                    </h4>
                    {{#if description}}
                        <div class="alert alert-info">{{md description}}</div>
                    {{/if}}

                    <!-- Nav tabs -->
                    <ul class="nav nav-tabs">
                        <li class="active">
                            <a href="#{{method}}_request_{{../uniqueId}}" data-toggle="tab">Request</a>
                        </li>
                        {{#if responses}}
                            <li>
                                <a href="#{{method}}_response_{{../../uniqueId}}" data-toggle="tab">Response</a>
                            </li>
                        {{/if}}
                    </ul>

                    <!-- Tab panes -->
                    <div class="tab-content">
                        <div class="tab-pane active" id="{{method}}_request_{{../uniqueId}}">
                            {{#if ../allUriParameters}}
                                <h3>URI Parameters</h3>
                                <ul>
                                    {{#each ../../allUriParameters}}
                                        {{> item}}
                                    {{/each}}
                                </ul>
                            {{/if}}

                            {{#if headers}}
                                <h3>Headers</h3>
                                <ul>
                                    {{#each headers}}
                                        {{> item}}
                                    {{/each}}
                                </ul>
                            {{/if}}

                            {{#if queryParameters}}
                                <h3>Query Parameters</h3>
                                <ul>
                                    {{#each queryParameters}}
                                        {{> item}}
                                    {{/each}}
                                </ul>
                            {{/if}}

                            {{#if body}}
                                <h3>Body</h3>
                                {{#each body}}
                                    <p><strong>Type: {{@key}}</strong></p>
                                    {{#if formParameters}}
                                        <strong>Form Parameters</strong>
                                        <ul>
                                            {{#each formParameters}}
                                                {{> item}}
                                            {{/each}}
                                        </ul>
                                    {{/if}}

                                    {{#if this.schema}}
                                        <p><strong>Schema</strong>:</p>
                                        <pre><code>{{this.schema}}</code></pre>
                                    {{/if}}

                                    {{#if this.example}}
                                        <p><strong>Example</strong>:</p>
                                        <pre><code>{{this.example}}</code></pre>
                                    {{/if}}
                                {{/each}}
                            {{/if}}
                        </div>

                        {{#if responses}}
                            <div class="tab-pane" id="{{method}}_response_{{../../uniqueId}}">
                                {{#each responses}}
                                    <h2>HTTP status code <a href="http://httpstatus.es/{{@key}}" target="_blank">{{@key}}</a></h2>
                                    {{md this.description}}

                                    {{#if this.headers}}
                                        <h3>Headers</h3>
                                        <ul>
                                            {{#each this.headers}}
                                                {{> item}}
                                            {{/each}}
                                        </ul>
                                    {{/if}}

                                    {{#if this.body}}
                                        <h3>Body</h3>
                                        {{#each this.body}}
                                            <p><strong>Type: {{@key}}</strong></p>

                                            {{#if this.schema}}
                                                <p><strong>Schema</strong>:</p>
                                                <pre><code>{{this.schema}}</code></pre>
                                            {{/if}}

                                            {{#if this.example}}
                                                <p><strong>Example</strong>:</p>
                                                <pre><code>{{this.example}}</code></pre>
                                            {{/if}}
                                        {{/each}}
                                    {{/if}}
                                {{/each}}
                            </div>
                        {{/if}}
                    </div>
                </div>
            {{/methods}}
        </div>
    </div>
{{/emptyResourceCheck}}

{{#resources}}
    {{> resource}}
{{/resources}}
